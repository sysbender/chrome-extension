{
  "name": "ext1",
  "version": "1.0.0",
  "description": "first",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["content.js", "contentMessaging.js"],
      "matches": ["https://*.youtube.com/*"],
      "exclude_matches": [],
      "run_at": "document_end"
    }
  ],

  "permissions": ["bookmarks", "storage", "activeTab", "contextMenus"],
  "icons": {
    "16": "icon-m.png",
    "48": "icon-m.png",
    "128": "icon-m.png"
  }
}
